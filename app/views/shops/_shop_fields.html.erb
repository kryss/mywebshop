<%= render 'shared/error_messages', :object => f.object %>
<div class="field">
    <%= f.inputs do %>
      <%= f.input :name %>
      <%= f.input :address %>
      <%= f.input :city %>
      <%= f.input :zip_code, :as => :numeric %>
      <%= f.input :country, :as => :string %>
      <%= f.input :phone %>
      <%= f.input :logo %>
      <%= f.input :description, :as => :text %>
    <% end %>

</div>
