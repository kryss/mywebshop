<!DOCTYPE html>
<html>
<head>
  <title><%= @title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
</head>
<body>
<div class="container">
  <%= render 'layouts/header' %>
  <%= yield %>

  <%= render 'layouts/footer' %>
  <br/>
  <br/>
  <%= debug(params) if Rails.env.development? %>
  <%= debug(session) if Rails.env.development? %>
</div>
</body>
</html>
